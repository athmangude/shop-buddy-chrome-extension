{
  "version": "0.2.5",
  "name": "Shopbuddy",
  "manifest_version": 2,
  "description": "You shop, we deliver",
  "browser_action": {
    "default_title": "Shopbuddy",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "key": "",
  "web_accessible_resources": [
    "inject.html"
  ],
  "content_scripts": [
    {
      "js": [
        "libs/jquery-3.0.0.min.js"
      ],
      "matches": [
        "https://www.amazon.com/*"
      ]
    }
  ],
  "externally_connectable": {
    "matches": [
      "chrome-extension://jllbjccoodgjljdmpijpmkhmdkndamho/window.html"
    ]
  },
  "background": {
    "page": "background.html",
    "js": [
      "libs/jquery-3.0.0.min.js"
    ],
    "matches": [
      "https://www.amazon.com/*"
    ]
  },
  "oauth2": {
    "client_id": "533443841306-ufahtm8dbselgnr543vi509eqs3si8ro.apps.googleusercontent.com",
    "scopes": [
      "profile",
      "email"
    ]
  },
  "permissions": [
    "identity",
    "identity.email",
    "contextMenus",
    "management",
    "tabs",
    "activeTab",
    "storage",
    "https://www.amazon.com/*",
    "http://ecs.amazonaws.com/*"
  ],
  "content_security_policy": "default-src 'self'; frame-src *; script-src 'self' https://apis.google.com https://54.200.181.243 https://webservices.amazon.com 'unsafe-eval'; object-src 'self'; connect-src *; style-src * 'unsafe-inline'; img-src * 'self' data:; font-src *;"
}
